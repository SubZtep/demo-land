{
  "name": "demo-land",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nollup --environment NODE_ENV:development -c -w",
    "dev-build": "npm run generate-imports && rollup --environment NODE_ENV:development -c",
    "build": "rollup --environment NODE_ENV:production -c",
    "test": "jest",
    "lint": "eslint .",
    "wipe": "bash ./scripts/wipe.sh",
    "generate-imports": "ts-node scripts/generate-imports.ts",
    "postinstall": "npm run generate-imports && bash ./scripts/init.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@open-wc/rollup-plugin-html": "^1.2.5",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-strip": "^2.0.0",
    "@types/aframe": "^1.0.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.8.1",
    "aframe": "^1.0.4",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.1",
    "nollup": "^0.13.13",
    "prettier": "^2.1.2",
    "pug": "^3.0.0",
    "rollup": "^2.32.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^9.0.0",
    "typescript": "~4.0.5"
  }
